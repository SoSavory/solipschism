{
  "manifest_version": 2,

  "name": "Solipschism",
  "description": "Allows people to share themselves.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Solipschism"
  },
  "permissions": [
    "storage",
    "alarms",
    "geolocation"
  ],
  "background": {
    "scripts": ["js/jquery-3.1.1.min.js","js/main.js","js/background.js"]
  }
}
