{
  "manifest_version": 2,

  "name": "Solipschism",
  "description": "Allows people to share themselves.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Solipschism"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}
